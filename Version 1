    // Populate dropdowns
    function populateDropdowns() {
        const dropdowns = [
            document.getElementById('player1'),
            document.getElementById('player2'),
            document.getElementById('player3'),
            document.getElementById('player4'),
            document.getElementById('player5'),
            document.getElementById('player6')
        ];
        dropdowns.forEach(dropdown => {
            players.forEach(player => {
                const option = document.createElement('option');
                option.value = player.PLAYER_NAME;
                option.text = player.PLAYER_NAME;
                dropdown.add(option);
            });
        });
    }

    // Generate teams based on selected players
    function generateTeams() {
        const selectedPlayers = [
            document.getElementById('player1').value,
            document.getElementById('player2').value,
            document.getElementById('player3').value,
            document.getElementById('player4').value,
            document.getElementById('player5').value,
            document.getElementById('player6').value
        ];

        const selectedPlayerObjects = selectedPlayers.map(name => players.find(player => player.PLAYER_NAME === name));
        selectedPlayerObjects.sort((a, b) => b.PLAYER_VALUE - a.PLAYER_VALUE); // Sort players by value descending

        const team1 = [];
        const team2 = [];
        let team1Value = 0;
        let team2Value = 0;

        selectedPlayerObjects.forEach(player => {
            if (team1Value <= team2Value) {
                team1.push(player);
                team1Value += player.PLAYER_VALUE;
            } else {
                team2.push(player);
                team2Value += player.PLAYER_VALUE;
            }
        });

        const now = new Date();
        const time = now.toLocaleTimeString();

        const output = `
            <p>Team 1: ${team1.map(player => player.PLAYER_NAME).join(', ')} (Total Value: ${team1Value})</p>
            <p>Team 2: ${team2.map(player => player.PLAYER_NAME).join(', ')} (Total Value: ${team2Value})</p>
            <p>Generated at: ${time}</p>
        `;
        document.getElementById('output').innerHTML = output;
    }

    // Initialize dropdowns on page load
    window.onload = populateDropdowns;
</script>

